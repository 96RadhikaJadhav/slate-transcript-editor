    "bootstrap-css-only": "^4.4.1",
    "docx": "4.7.1",
    "draft-js": "^0.11.5",
    "gh-pages": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.once": "^4.1.1",
    "lodash.throttle": "^4.1.1",
    "react-bootstrap": "^1.0.1",
